<% provide(:title, 'All users') %>
<h1>All users</h1>

<%= form_tag users_path, method: :get, remote: :true do %> 
  <p>
    <%= text_field_tag :search%>
    <%= submit_tag "Search", class: "btn btn-primary"  %>
  </p>
<% end %>

<div class="paginated-users">
	<%= render "paginated-users" %>
</div>


